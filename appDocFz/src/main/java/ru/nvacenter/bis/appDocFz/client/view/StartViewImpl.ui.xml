	<ui:with field="resources"
		type="ru.nvacenter.bis.appDocFz.client.resources.Resources" />
	<g:ScrollPanel>
		<g:FlowPanel>
			<g:CaptionPanel captionText="Администратор" ui:field="adminPanel"
